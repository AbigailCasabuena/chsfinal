@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <table class="table table-striped">
        <tr>
            <thead>LRN</thead>
            <thead>First Name</thead>
            <thead>Middle Name</thead>
            <thead>Last Name</thead>
            <thead>Status</thead>
            <thead>Grade Level</thead>
        </tr>
        <tbody>
            @if (ViewBag.studentList != null)
            {
                foreach (var item in ViewBag.studentList)
                {
                    <tr>
                        <td>@item.lrn</td>
                        <td>@item.fname</td>
                        <td>@item.mname</td>
                        <td>@item.lname</td>
                        <td>@item.status</td>
                        <td>@item.grade_level</td>
                        <td><a href="#"><i class="glyphicon glyphicon-eye-open"></i>View</a></td>
                    </tr>
                }
            }
        </tbody>
    </table>
   
</div>